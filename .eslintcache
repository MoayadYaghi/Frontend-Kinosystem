essum: true });\r\n    this.setState({ Datenschutz: false });\r\n  }\r\n\r\n  DatenschutzHandler() {\r\n    this.setState({ AGB: false });\r\n    this.setState({ Impressum: false });\r\n    this.setState({ Datenschutz: true });\r\n  } */\r\n\r\n  scrollToTop = () => {\r\n    scroll.scrollToTop();\r\n  };\r\n\r\n  render() {\r\n    return (\r\n      <div className=\"Flex\">\r\n\r\n<view className=\"Tabelle\">\r\n            <div className=\"Hintergrund\">\r\n              <div className=\"SpalteBL\">\r\n                <Link\r\n                  className=\"SpalteW\"\r\n                  activeClass=\"active\"\r\n                  to=\"Impressum\"\r\n                  spy={true}\r\n                  smooth={true}\r\n                  offset={-70}\r\n                  duration={800}\r\n                >\r\n                  Impressum\r\n                </Link>\r\n              </div>\r\n              <div className=\"SpalteBL\">\r\n                <Link\r\n                  className=\"SpalteS\"\r\n                  activeClass=\"active\"\r\n                  to=\"Datenschutz\"\r\n                  spy={true}\r\n                  smooth={true}\r\n                  offset={-70}\r\n                  duration={3000}\r\n                >\r\n                  Datenschutz\r\n                </Link>\r\n              </div>\r\n              <div className=\"SpalteBL\">\r\n                <Link\r\n                  className=\"SpalteW\"\r\n                  activeClass=\"active\"\r\n                  to=\"AGB\"\r\n                  \r\n                  spy={true}\r\n                  smooth={true}\r\n                  offset={-70}\r\n                  duration={4000}\r\n                >\r\n                  AGB\r\n                </Link>\r\n              </div>\r\n              \r\n            </div>\r\n          </view>\r\n\r\n          <div>\r\n            <Impressum />\r\n            <div className=\"BacktoTop\" onClick={this.scrollToTop}>\r\n              {\" \"}\r\n    