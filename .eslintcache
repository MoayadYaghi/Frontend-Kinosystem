}\r\n        {this.state.EmailSupport ? (\r\n          <div className=\"AnmeldungFehler\">\r\n            <div className=\"DESIGNHeadline3\">\r\n              Bitte geben Sie eine korrekte EMail Adresse ein\r\n            </div>\r\n          </div>\r\n        ) : null}\r\n        {this.state.passwortÜber ? (\r\n          <div className=\"AnmeldungFehler\">\r\n            <div className=\"DESIGNHeadline3\">\r\n              Die Passwörter stimmen nicht überein, bitte versuchen Sie es\r\n              erneut\r\n            </div>\r\n          </div>\r\n        ) : null}\r\n\r\n        {this.state.Fehler ? (\r\n          <div className=\"AnmeldungFehler\">\r\n            {\" \"}\r\n            <div className=\"DESIGNTextField\">\r\n              Ihre Anmeldung war fehlerhaft, bitte überprüfen Sie ihre Eingaben\r\n            </div>\r\n          </div>\r\n        ) : null}\r\n\r\n        <br />\r\n        {this.state.visible ? (\r\n          <form className=\"FormFenster\" onSubmit={this.handleSubmit}>\r\n            <div className=\"InputField\">\r\n              {/* <label>\r\n              <select\r\n                className=\"Textfielde\"\r\n                value={this.state.value}\r\n                onChange={this.handleChange}\r\n                name=\"Anrede\"\r\n              >\r\n                <option value=\"PreisKategorie\">Anrede Auswählen</option>\r\n                <option value=\"Herr\">Herr</option>\r\n                <option value=\"Frau\">Frau</option>\r\n                <option value=\"Divers\">Divers</option>\r\n                \r\n              </select>\r\n            </label> */}\r\n\r\n              <div className=\"Zusammenhängend\">\r\n                <label>\r\n                  <input\r\n                    className=\"Textfieldd\"\r\n                    placeholder=\"Vorname\"\r\n                    name=\"vorname\"\r\n                    type=\"text\"\r\n                    value={this.state.vorname}\r\n                    onChange={this.handleChange}\r\n                  />\r\n                </label>\r\n                <label>\r\n                  <input\r\n                    className=\"Textfieldd\"\r\n                    placeholder=\"Nachname\"\r\n                    name=\"nachname\"\r\n                    type=\"text\"\r\n                    value={this.state.nachname}\r\n                    onChange={this.handleChange}\r\n                  />\r\n                </label>\r\n              </div>\r\n\r\n              <label>\r\n                <input\r\n                  className=\"Textfielde\"\r\n                  placeholder=\"E-Mail Adresse\"\r\n                  name=\"email\"\r\n                  type=\"text\"\r\n                  value={this.state.email}\r\n                  onChange={this.handleChange}\r\n                />\r\n              </label>\r\n\r\n              <label>\r\n                <input\r\n                  className=\"Textfielde\"\r\n                  placeholder=\"Benutzername\"\r\n                  name=\"username\"\r\n                  type=\"text\"\r\n                  value={this.state.username}\r\n                  onChange={this.handleChange}\r\n                />\r\n              </label>\r\n              <label>\r\n                <input\r\n                  className=\"Textfielde\"\r\n                  placeholder=\"Passwort\"\r\n                  name=\"passwortHash\"\r\n                  type={this.state.passwortButtonShow}\r\n                  value={this.state.passwortHash}\r\n                  onChange={this.handleChange}\r\n                />\r\n              </label>\r\n\r\n              <label>\r\n                <input\r\n                  className=\"Textfielde\"\r\n                  placeholder=\"Passwort Wiederholen\"\r\n                  name=\"Passwort2\"\r\n                  type={this.state.passwortButtonShow}\r\n                  value={this.state.Passwort2}\r\n                  onChange={this.handleChange}\r\n                />\r\n              </label>\r\n              <div className=\"PasswortInfo\">\r\n                <div className=\"DESIGNTextField\">\r\n                  Das Passwort muss aus mindestens 8 Zeichen bestehen und eine\r\n                  Zahl beinhalten\r\n                  <br />\r\n                </div>{\" \"}\r\n              </div>\r\n              <div className=\"ButtonPasswortDiv\">\r\n                <div className=\"DESIGNButton\" onClick={this.showPass}>\r\n                  <div className=\"ButtonPasswort\">Show Password</div>\r\n                </div>\r\n              </div>\r\n              <label>\r\n                <select\r\n                  className=\"Textfielde\"\r\n                  value={this.state.preiskategorie}\r\n                  onChange={this.handleChange}\r\n                  name=\"preiskategorie\"\r\n                >\r\n                  <option value=\"default\">Preis Kategorie Auswählen</option>\r\n                  <option value=\"ERWACHSENER\">Erwachsener</option>\r\n                  <option value=\"STUDIEREND\">Student</option>\r\n                  <option value=\"KIND\">Kind</option>\r\n                  <option value=\"SENIOR\">Senior</option>\r\n                  <option value=\"MENSCH_MIT_BEHINDERUNG\">\r\n                    Mensch mit Behinderung\r\n                  </option>\r\n                  <option value=\"BEGLEITPERSON\">Begleitperson</option>\r\n                </select>\r\n              </label>\r\n\r\n              <div className=\"SitzplatzAuswahl\">\r\n                <div className=\"DESIGNHeadline3\">\r\n                  {\" \"}\r\n                  Bitte Wählen sie eine favorisierten Sitzplatz Region aus\r\n                </div>\r\n\r\n                <div className=\"Leinwand\">\r\n                  <div className=\"DESIGNHeadline3\">Kino Leinwand</div>\r\n                </div>\r\n                <div className=\"FlexboxDesign\">\r\n                  <div\r\n                    className={this.state.BG1}\r\n                    onClick={() => this.BoxAuswahl(1, \"VORNE_LINKS\")}\r\n                  >\r\n                    Vorne Links\r\n                  </div>\r\n                  <div\r\n                    className={this.state.BG2}\r\n                    onClick={() => this.BoxAuswahl(2, \"VORNE_MITTE\")}\r\n                  >\r\n                    Vorne Mitte\r\n                  </div>\r\n                  <div\r\n                    className={this.state.BG3}\r\n                    onClick={() => this.BoxAuswahl(3, \"VORNE_RECHTS\")}\r\n                  >\r\n                    Vorne Rechts\r\n                  </div>\r\n                </div>\r\n                <div className=\"FlexboxDesign\">\r\n                  <div\r\n                    className={this.state.BG4}\r\n                    onClick={() => this.BoxAuswahl(4, \"MITTE_LINKS\")}\r\n                  >\r\n                    Mitte &nbsp;Links\r\n                  </div>\r\n                  <div\r\n                    className={this.state.BG5}\r\n                    onClick={() => this.BoxAuswahl(5, \"MITTE_MITTE\")}\r\n                  >\r\n                    Mitte{\" \"}\r\n                  </div>\r\n                  <div\r\n                    className={this.state.BG6}\r\n                    onClick={() => this.BoxAuswahl(6, \"MITTE_RECHTS\")}\r\n                  >\r\n                    Mitte Rechts\r\n                  </div>\r\n                </div>\r\n                <div className=\"FlexboxDesign\">\r\n                  <div\r\n                    className={this.state.BG7}\r\n                    onClick={() => this.BoxAuswahl(7, \"HINTEN_LINKS\")}\r\n                  >\r\n                    Hinten Links\r\n                  </div>\r\n                  <div\r\n                    className={this.state.BG8}\r\n                    onClick={() => this.BoxAuswahl(8, \"HINTEN_MITTE\")}\r\n                  >\r\n                    Hinten Mitte\r\n                  </div>\r\n                  <div\r\n                    className={this.state.BG9}\r\n                    onClick={() => this.BoxAuswahl(9, \"HINTEN_RECHTS\")}\r\n                  >\r\n                    Hinten Rechts\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div className=\"newsletterField\">\r\n              <label>\r\n                <div className=\"DESIGNTextField\">\r\n                  <input\r\n                    name=\"newsletter\"\r\n                    type=\"checkbox\"\r\n                    checked={this.state.newsletter}\r\n                    onChange={this.handleChange}\r\n                  />\r\n                  &ensp; Ich möchte den Newsletter abonnieren\r\n                </div>\r\n              </label>\r\n            </div>\r\n            <br />\r\n            <div className=\"SubmitField\">\r\n              <input\r\n                className=\"DESIGNButton\"\r\n                type=\"submit\"\r\n                value=\" Registrieren\"\r\n              />\r\n            </div>\r\n          </form>\r\n        ) : (\r\n          <div>\r\n            <div className=\"Erfolgreich\">\r\n              {\" \"}\r\n              <div className=\"DESIGNHeadline3\">\r\n                Sie haben sich erfolgreich angemeldet{\" \"}\r\n              </div>\r\n            </div>\r\n            <div className=\"LoginLinks\">\r\n              <Link className=\"DESIGNButton\" to={Startseite}>\r\n                Startseite\r\n              </Link>\r\n\r\n              <Link className=\"DESIGNButton\" to={LogInLink}>\r\n                Login\r\n              </Link>\r\n            </div>\r\n          </div>\r\n        )}\r\n      </div>\r\n    );\r\n  }\r\n}\r\nexport default Anmeldung;\r\n","C:\\Users\\Cevin\\Desktop\\Frontend\\Frontend-Kinosystem\\src\\Components\\ProgrammSeite\\ProgrammSeite.js",["383","384","385","386","387","388","389","390"],"import React, { Component } from \"react\";\r\nimport \"./Programm.css\";\r\n\r\nimport { Link } from \"react-router-dom\";\r\nimport GetAllFilmAPI from \"../../API_Pulls/GetAllFilmAPI\";\r\nimport { Redirect } from 'react-router-dom';\r\n\r\nclass ProgrammSeite extends Component {\r\n  constructor(props) {\r\n    super(props);\r\n    this.state = {\r\n      headline: 'Ergebnisse',\r\n      genre: 'Alle',\r\n      alter: 'Alle',\r\n      filmName:\" \",\r\n      search: \"\",\r\n      showFilter: false,\r\n      redirect: false,\r\n      filmId: \"\",\r\n      alleFilme: [],\r\n      filme: []};\r\n\r\n    this.handleChange = this.handleChange.bind(this);\r\n    this.hideFilter = this.hideFilter.bind(this);\r\n    this.setRedirect = this.setRedirect.bind(this);\r\n  }\r\n\r\n  componentDidMount() {\r\n    GetAllFilmAPI.getAllFilmAPI().then((response) => {\r\n\r\n      this.setState({ filme: response.data });\r\n      this.setState({ alleFilme: response.data });\r\n      console.log(this.state.filme);\r\n    });\r\n  }\r\n\r\n  handleChange(event) {\r\n    if(event.target.id == \"searchId\") {\r\n      this.setState({search: event.target.value})\r\n    }\r\n    else if(event.target.id == \"genreId\") {\r\n      this.setState({genre: event.target.value});\r\n    }\r\n    else if(event.target.id == \"alterId\") {\r\n      this.setState({alter: event.target.value});\r\n    }\r\n\r\n    let alleFilme = this.state.alleFilme;\r\n\r\n    let filmCounter = 0;\r\n    let genreFilme = [];\r\n    let alterFilme = [];\r\n    let suchFilme = [];\r\n    let genre = document.getElementById(\"genreId\");\r\n    let alter = document.getElementById(\"alterId\");\r\n    let search = document.getElementById(\"searchId\")\r\n\r\n    for(let film in alleFilme) {\r\n      if(alleFilme[film].name.includes(search.value)) {\r\n        suchFilme.push(alleFilme[film]);\r\n      }\r\n    }\r\n    //this.setState({ filme: suchFilme})\r\n    if(this.state.showFilter === true) {\r\n      if(genre.value == \"Alle\") {\r\n        genreFilme = suchFilme;\r\n      } else {\r\n        for(let i=0; i<suchFilme.length; i++) {\r\n          let genreList = suchFilme[i].genre;\r\n          for(let g in genreList) {\r\n  \r\n            if(genreList[g]== genre.value) {\r\n              genreFilme.push(suchFilme[i]);\r\n              break;\r\n            }\r\n          }\r\n        }\r\n      }\r\n      if(alter.value == \"Alle\") {\r\n        alterFilme = genreFilme;\r\n      } else {\r\n        for(let i=0; i<genreFilme.length; i++) {\r\n          let altersfreigabe = genreFilme[i].mindestAlter;\r\n          if(altersfreigabe <= alter.value) {\r\n            alterFilme.push(genreFilme[i]);\r\n          }\r\n        }\r\n      }\r\n      this.setState({ filme: alterFilme });\r\n      filmCounter = alterFilme.length;\r\n    } else {\r\n      this.setState({ filme: suchFilme });\r\n      filmCounter = suchFilme.length;\r\n    }\r\n    if(filmCounter > 0) {\r\n      this.setState({ headline: 'Ergebnisse'});\r\n    } else {\r\n      this.setState({ headline: 'Zu diesen Suchkriterien gibt es keine Filme'});\r\n    }\r\n  }\r\n\r\n  hideFilter() {\r\n    this.setState({ showFilter: !this.state.showFilter });\r\n  }\r\n\r\n  setRedirect = (event) => {\r\n    let id = event.target.id;\r\n    this.setState({redirect: true});\r\n    this.setState({filmId: id});\r\n  }\r\n  renderRedirect = () => {\r\n    if (this.state.redirect) {\r\n      return <Redirect to={`/FilmDetails/${this.state.filmId}`} />\r\n    }\r\n  }\r\n\r\n  render() {\r\n    const { showFilter } = this.state;\r\n\r\n    return (\r\n      <div className = \"\" >\r\n        <div className = \"wholeFilterSection\">\r\n          <div className = \"searchBar\">\r\n            <input className = \"searchField\" id=\"searchId\" type=\"text\" placeholder=\"Search\" value={this.state.search} onChange={this.handleChange}/>\r\n          </div>\r\n          <div className = \"filter\">\r\n            <button onClick={() => this.hideFilter()}>\r\n              Filter\r\n            </button>\r\n            {showFilter && <div className = \"genre\">\r\n              <form>\r\n              <label>\r\n                Genre\r\n                <select id=\"genreId\" value={this.state.genre} onChange={this.handleChange}>\r\n                  <option value=\"Alle\">Alle</option>\r\n                  <option value=\"Action\">Action</option>\r\n                  <option value=\"Drama\">Drama</option>\r\n                  <option value=\"Sci-Fi\">Sci-Fi</option>\r\n                  <option value=\"Fantasy\">Fantasy</option>\r\n                  <option value=\"Horror\">Horror</option>\r\n                  <option value=\"Animation\">Animation</option>\r\n                  <option value=\"Family\">Familie</option>\r\n                </select>\r\n              </label>\r\n            </form>\r\n            </div>}\r\n            {showFilter && <div className = \"alter\">\r\n              <form>\r\n              <label>\r\n                Altersfreigabe \r\n                <select id=\"alterId\" value={this.state.alter} onChange={this.handleChange}>\r\n                  <option value=\"Alle\">Alle</option>\r\n                  <option value=\"6\">ab 6 Jahren</option>\r\n                  <option value=\"12\">ab 12 Jahren</option>\r\n                  <option value=\"16\">ab 16 Jahren</option>\r\n                  <option value=\"18\">ab 18 Jahren</option>\r\n                </select>\r\n              </label>\r\n            </form>\r\n            </div>}\r\n          </div>\r\n        </div>\r\n        <h1 className=\"text-center\"> {this.state.headline} </h1>\r\n        <br />\r\n        <div className=\"filmListe\">\r\n          {this.state.filme.map((film) => (\r\n          <div className=\"\" key={film.id}>\r\n            <div className=\"ErgebnisDarstellung\">\r\n              <img key={film.id} alt=\" \" className=\"image\" src={film.bild}/>\r\n            </div>\r\n            {this.renderRedirect()}\r\n            <div id={film.id} className=\"Title\"